{
  "name": "vue3_vite4_ts_pinia_template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "lint": "eslint src --no-fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:fix": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "style-lint": "stylelint src/**/*.{css,sass,scss,less,vue}",
    "style-lint:fix": "stylelint src/**/*.{css,sass,scss,less,vue} --fix",
    "prepare": ""
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.{ts,tsx,vue,js,jsx}": [
      "npm run lint"
    ],
    "*.{css,sass,scss,less,vue}": [
      "npm run style-lint"
    ]
  },
  "prettier": {
    "semi": false
  }
}
